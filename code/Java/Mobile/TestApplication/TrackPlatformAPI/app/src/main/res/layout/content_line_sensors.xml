<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.yuras.trackplatformapi.LineSensorsActivity"
    tools:showIn="@layout/activity_line_sensors">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="First sensor: "
        android:textSize="24sp"
        android:id="@+id/textView"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="44dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Second sensor: "
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/textView2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Third sensor: "
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="22dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fourth sensor: "
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/textView4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fifth sensor: "
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="21dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/first_line_sensor_tv"
        android:layout_width="105dp"
        android:layout_height="30dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteY="44dp"
        tools:layout_editor_absoluteX="290dp" />

    <TextView
        android:id="@+id/second_line_sensor_tv"
        android:layout_width="105dp"
        android:layout_height="30dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteY="103dp"
        tools:layout_editor_absoluteX="290dp" />

    <TextView
        android:id="@+id/third_line_sensor_tv"
        android:layout_width="102dp"
        android:layout_height="32dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteY="153dp"
        tools:layout_editor_absoluteX="293dp" />

    <TextView
        android:id="@+id/fourth_line_sensor_tv"
        android:layout_width="102dp"
        android:layout_height="29dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteY="209dp"
        tools:layout_editor_absoluteX="293dp" />

    <TextView
        android:id="@+id/fifth_line_sensor_tv"
        android:layout_width="102dp"
        android:layout_height="31dp"
        android:textSize="24sp"
        tools:layout_editor_absoluteY="258dp"
        tools:layout_editor_absoluteX="293dp" />

</android.support.constraint.ConstraintLayout>
